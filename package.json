{
  "name": "sailfish-page-next",
  "version": "0.1.1",
  "description": "sailfish-page-next",
  "main": "dist/sailfish-page-next.min.js",
  "module": "dist/sailfish-page-next.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "npm run type-check && npm run build:dev && npm run build:type",
    "build:dev": "gulp lint && cross-env NODE_ENV=development rollup -c",
    "build:prod": "gulp lint && cross-env NODE_ENV=production rollup -c",
    "build:type": "tsc --emitDeclarationOnly",
    "build": "npm run type-check && npm run build:prod && npm run build:type",
    "lint": "gulp lint",
    "lint:test": "gulp lintTest",
    "type-check": "tsc --noEmit",
    "karma": "npm run lint:test && karma start ./test/karma.conf.js",
    "prepublishOnly": "npm run karma && npm run build",
    "lint:js": "eslint --ext .js,.ts --fix --format node_modules/eslint-friendly-formatter src test/unit"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": ">=3.0.4",
    "vue": ">=3.0.4"
  },
  "author": "harrisyang",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.7.7",
    "@commitlint/cli": "^9.1.1",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/mockjs": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^3.2.0",
    "del": "^5.0.0",
    "eslint": "^6.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "husky": "^4.2.5",
    "karma": "^4.2.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage": "^1.1.2",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^7.0.2",
    "lint-staged": "^10.2.11",
    "mocha": "^6.2.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.0.5",
    "rollup": "^1.26.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-typescript2": "^0.25.3",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.5.5",
    "vue": "^3.0.4",
    "vue-class-component": "^7.2.6"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "*.+(js|ts|json)": [
      "prettier --write"
    ],
    "*.+(js|ts)": [
      "npm run lint:js",
      "git add"
    ]
  }
}
